  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceRoot}/injector",
      "buildFlags": "-cover",
      "args": [
        "-test.v",
        "-test.run",
        "./...",
        "-test.coverprofile",
        "${workspaceRoot}/cover.out",
        "-test.testlogfile",
        "${workspaceRoot}/cover.log"
      ],
      "showLog": true,
    }
  ]
}
